<Page x:Class="CHW_ATP.AuthorizePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CHW_ATP"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="AuthorizePage">

    <Grid>
        <PasswordBox x:Name="passwordBox" Margin="120,55,10,0" VerticalAlignment="Top" Height="25"/>
        <TextBox x:Name="textBox_Login" Height="25" Margin="120,10,10,0" 
                  VerticalAlignment="Top"/>
        <Label x:Name="label_Login" Content="Login:" Margin="23,10,211,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Password" Content="Password:" Margin="20,55,198,0" VerticalAlignment="Top"/>
        <Button x:Name="button_register" Content="Authorize" Margin="100,100,10,0" 
                VerticalAlignment="Top" Width="120" Height="25" HorizontalAlignment="Center" Click="button_register_Click"/>
        


    </Grid>
</Page>
