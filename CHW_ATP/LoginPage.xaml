<Page x:Class="CHW_ATP.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CHW_ATP"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="LoginPage">
    <Grid>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="90,147,0,0" VerticalAlignment="Top" Width="98"/>
        <TextBox x:Name="textBox_Login" HorizontalAlignment="Left" Height="23" Margin="90,103,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label_Login" Content="Login" HorizontalAlignment="Left" Margin="23,103,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Password" Content="Password" HorizontalAlignment="Left" Margin="23,139,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button_register" Content="Create account" HorizontalAlignment="Left" Margin="31,195,0,0" VerticalAlignment="Top" Width="104" Click="button_register_Click"/>
        
    </Grid>
    
</Page>
